import numpy as np
import matplotlib.pyplot as plt
# Generate x values
x = np.linspace(-2 * np.pi, 2 * np.pi, 1000)
# Calculate corresponding y values for sin(x)
y_sin = np.sin(x)
# Plot the sine function
plt.plot(x, y_sin, label='y = sin(x)')
# Customize the plot
plt.title('Plot of y = sin(x)')
plt.xlabel('x')
plt.ylabel('y')
plt.grid(True)
plt.legend()
# Show the plot
plt.show()
